<div class="images">
  <h1><%= object.title %></h1>

  <% object.gallery_pictures.each do |picture| %>
    <div class="col-xs-6 col-md-3">
      <%= link_to (image_tag picture.picture_url.sub(/\d+-\d+/,"230-129!m"), :class => 'img-responsive'),
        picture.picture_url, :class => 'thumbnail js-colorbox', :rel => 'colorbox' %>
    </div>
  <% end  %>
</div>
